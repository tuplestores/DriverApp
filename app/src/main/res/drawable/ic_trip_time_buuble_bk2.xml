<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:right="-25dp"
        >

        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <solid android:color="#ffffffff"></solid>
            <corners android:radius="35dp"></corners>
            <item android:name="android:shadowColor">#261069fd</item>
            <!-- TextView shadow offsets do not respect scale. -->
            <item android:name="android:shadowDx">0</item>
            <item android:name="android:shadowDy">2</item>
        </shape>

    </item>


</layer-list>
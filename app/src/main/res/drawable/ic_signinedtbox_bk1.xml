<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:right="-25dp"
        >

        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <solid android:color="#f5f9ff"></solid>
            <corners android:radius="25dp"></corners>
            <stroke android:color="#f2f7ff" android:width="1dp">
            </stroke>
            <gradient
                android:type="linear"
                android:startColor="#f2f7ff"
                android:endColor="#f6f9ff"
                android:angle="180"
                />
        </shape>

    </item>

    <item android:right="-25dp"
        android:left="5dp"
        android:top="5dp"
        android:bottom="5dp"

        >


        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle"
            >
            <solid android:color="@color/colorPrimary">
                </solid>
            <corners android:radius="25dp"></corners>
        </shape>
    </item>
</layer-list>